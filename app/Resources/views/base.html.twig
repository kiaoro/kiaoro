<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="">
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        <title>{% block title %}test{% endblock %}</title>
        {% block stylesheets %}
            <!--<link href='http://fonts.googleapis.com/css?family=Irish+Grover' rel='stylesheet' type='text/css'>-->
            <!--<link href='http://fonts.googleapis.com/css?family=La+Belle+Aurore' rel='stylesheet' type='text/css'>-->
            <link href="{{ asset('css/app.min.css') }}" rel="stylesheet" media="screen">
        {% endblock %}
        <script src="{{ asset('js/jquery.min.js') }}" type="text/javascript"></script>
    </head>
    <body>

    <div class="container">

        <!-- Static navbar -->
        {% block navigation %}
        <nav class="navbar navbar-default">
        <div class="container-fluid">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('NaissanceApplicationBundle_homepage') }}">TEST</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
                <li class="{% if app.request.get('_route') == 'NaissanceApplicationBundle_homepage' %}active{% endif %}"><a href="{{ path('NaissanceApplicationBundle_homepage') }}">Home</a></li>
                <li class="{% if app.request.get('_route') == 'NaissanceApplicationBundle_about' %}active{% endif %}"><a href="{{ path('NaissanceApplicationBundle_about') }}">About</a></li>
                <li class="{% if app.request.get('_route') == 'NaissanceApplicationBundle_contact' %}active{% endif %}"><a href="{{ path('NaissanceApplicationBundle_contact') }}">Contact</a></li>
                <li class="{% if app.request.get('_route') == 'fos_user_security_login' %}active{% endif %}"><a href="{{ path('fos_user_security_login') }}">Login</a></li>
            </ul>
        </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
        </nav>
        {% endblock %} 

        {% block body %}{% endblock %}

        <div>
            {% block sidebar %}{% endblock %}
        </div>

    </div> <!-- /container -->

    <footer class="footer">
        <div class="container">
            {% block footer %}
                <p class="text-muted">test tutorial - created by test</p>
            {% endblock %}
        </div>
    </footer>

    {% block javascripts %}
        <script src="{{ asset('js/app.min.js') }}" type="text/javascript"></script>
    {% endblock %}
    </body>
</html>